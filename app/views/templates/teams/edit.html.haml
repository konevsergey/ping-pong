.panel.panel-default
  .panel-heading
    .pull-left
      %h5 Select players
    .clearfix
  .panel-body
    %form{ :'ng-submit' => 'update()',
           :role => 'form',
           :name => 'form',
           :novalidate => true }

      =render 'templates/tournaments/select_players'


      .form-group
        =render_button 'submit', :'ng-disabled' => 'form.$invalid', text: 'Select'
        =render_button 'cancel', :'ui-sref' => '^'
